<% include ../partials/header %>
	 
	

<title>Flyable</title>

<body>
	<div class="container">
			<div>
				<a href="/users/logout">Log Out</a>
				<a href="/users/login">Login</a>
				<a href="/users/profile">Profile</a>
				<a href="/">Home</a>
			</div>
		</div>
		<h1>checking your weather station ...</h1>
	<div class="container">
		<div class="row">
			<div class="col-xs-6 col-sm-12"><%= wind_string %>
			</div>
			<div class="col-xs-6 col-sm-12"><%= wind_dir %>
			</div>
		</div>
		
		<div class="row">
			<div class="col-xs-6 col-sm-12"><%= dewpoint_string %></div>
			<div class="col-xs-6 col-sm-12"><%= wind_degrees %></div>
		</div>

		<div class="row">
			<div class="col-xs-6 col-sm-12"><%= temp_f %></div>
			<div class="col-xs-6 col-sm-12"><%= precip_today_string %></div>
		</div>
	</div>
	<img src="<%= cam_view %>" alt="The official HTML5 Icon"></img>


		
</body>
</html>

